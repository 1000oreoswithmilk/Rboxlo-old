<Window x:Class="RboxloLauncher.NewUpdate"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RboxloLauncher"
        mc:Ignorable="d"
        Title="NewUpdate" Height="450" Width="800">
    <Grid HorizontalAlignment="Right" Width="792" Height="419" VerticalAlignment="Bottom">
        <Image HorizontalAlignment="Left" Height="148" VerticalAlignment="Bottom" RenderOptions.BitmapScalingMode="HighQuality" Width="792" Source="/Resources/strip.png" Margin="0,0,0,271"/>
        <Label Content="A new update is available!" HorizontalAlignment="Left" Margin="92,0,0,310" VerticalAlignment="Bottom" Height="68" Width="599" FontSize="48" FontWeight="Bold" Foreground="White"/>
    </Grid>
</Window>
